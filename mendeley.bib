url = {http://revistas.unoeste.br/revistas/ojs/index.php/ca/article/viewArticle/1969},
volume = {13},
year = {2018}
}
@article{Baltokoski2010,
abstract = {O desenvolvimento de modelos hidrol{\'{o}}gicos capazes de predizer o impacto de fontes difusas de polui{\c{c}}{\~{a}}o e do uso e ocupa{\c{c}}{\~{a}}o do solo na qualidade das {\'{a}}guas superficiais e subterr{\^{a}}neas tem auxiliado o estudo de agroecossistemas. Com esse objetivo, foi utilizado o modelo SWAT 2005 (Soil and Water Assessment Tool) para avaliar sua sensibilidade na predi{\c{c}}{\~{a}}o da vaz{\~{a}}o e do fluxo de massa do P total. O estudo foi realizado em duas microbacias hidrogr{\'{a}}ficas cont{\'{i}}guas, dos rios Conrado e Pinheiro, afluentes do rio Pato Branco, localizadas nos munic{\'{i}}pios de Pato Branco e Mari{\'{o}}polis, no Estado do Paran{\'{a}}. Foram utilizados dados climatol{\'{o}}gicos do per{\'{i}}odo 1979/2006 e dados observados de vaz{\~{a}}o e concentra{\c{c}}{\~{a}}o de P total dos anos 2004/2005 de duas esta{\c{c}}{\~{o}}es de monitoramento, localizadas na parte inferior do curso principal dos rios Conrado e Pinheiro. Utilizou-se a interface AvSWAT{\_}X, com o SIG ArcView 3.3{\textregistered} e a extens{\~{a}}o Spatial Analyst 2.0{\textregistered}, para entrada e manipula{\c{c}}{\~{a}}o dos dados. As m{\'{e}}dias anuais e mensais observadas de vaz{\~{a}}o e P total foram comparadas aos dados simulados. O Coeficiente de Efici{\^{e}}ncia de Nash- Sutcliffe (COE) foi utilizado para avaliar a efici{\^{e}}ncia do modelo. A modelagem foi melhorada com a associa{\c{c}}{\~{a}}o de an{\'{a}}lise de sensibilidade, autocalibra{\c{c}}{\~{a}}o e calibra{\c{c}}{\~{a}}o manual, verificando-se que, com frequ{\^{e}}ncia de amostragem regular, o modelo SWAT 2005 realizou de forma aceit{\'{a}}vel as simula{\c{c}}{\~{o}}es de vaz{\~{a}}o e de exporta{\c{c}}{\~{a}}o de P total. J{\'{a}} com frequ{\^{e}}ncia de amostragem irregular e pequeno n{\'{u}}mero de dados, os procedimentos de an{\'{a}}lise de sensibilidade e de autocalibra{\c{c}}{\~{a}}o n{\~{a}}o foram eficientes na calibra{\c{c}}{\~{a}}o do modelo SWAT 2005 para a simula{\c{c}}{\~{a}}o de vaz{\~{a}}o e exporta{\c{c}}{\~{a}}o de P total. Foram encontrados diferentes n{\'{i}}veis de sensibilidade entre as duas esta{\c{c}}{\~{o}}es, refletindo as desigualdades entre as Unidades de Resposta Hidrol{\'{o}}gica. A distribui{\c{c}}{\~{a}}o mensal simulada das exporta{\c{c}}{\~{o}}es de P mostrou a heterogeneidade da aplica{\c{c}}{\~{a}}o do nutriente ao longo do ano.},
author = {Baltokoski, Valmir and Tavares, Maria Herm{\'{i}}nia Ferreira and Machado, Ronalton Evandro and de Oliveira, Marcio Paulo},
doi = {10.1590/S0100-06832010000100026},
isbn = {0100-0683},
issn = {01000683},
journal = {Revista Brasileira de Ciencia do Solo},
keywords = {Geotechnologies,Hydrologic modeling,Nonpoint source pollution,Phosphorous transport,Water quality},
number = {1},
pages = {253--261},
title = {{Calibra????o de modelo para a simula????o de vaz??o e de f??sforo total nas sub-bacias dos rios Conrado e Pinheiro - Pato Branco (PR)}},
volume = {34},
year = {2010}
}
