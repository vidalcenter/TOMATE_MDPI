@article{Graca2015,
abstract = {O Edital de Coopera{\c{c}}{\~{a}}o Brasil-Mo{\c{c}}ambique, lan{\c{c}}ado anualmente pelo CNPq, tem permitido a mo{\c{c}}ambicanos cursarem p{\'{o}}s-gradua{\c{c}}{\~{a}}o  stricto sensu  no Brasil com bolsa concedida por este pa{\'{i}}s, de forma a contribuir para o desenvolvimento s{\'{o}}cio-econ{\^{o}}mico de Mo{\c{c}}ambique. Entre as culturas de relev{\^{a}}ncia para Mo{\c{c}}ambique tem-se o tomateiro, cujo melhoramento para a mesa e para a ind{\'{u}}stria {\'{e}} de relevante import{\^{a}}ncia para que Mo{\c{c}}ambique alavanque a produ{\c{c}}{\~{a}}o com forte impacto no desenvolvimento do pa{\'{i}}s. Neste intento, realizou-se o cruzamento dial{\'{e}}lico completo, sem rec{\'{i}}procos entre cinco linhagens (‘B13LD', ‘Castone', ‘Massag-72', ‘Viradouro' e ‘Rio Grande') e estimaram-se as capacidades geral (CGC) e espec{\'{i}}fica (CEC) de combina{\c{c}}{\~{a}}o, bem como as heteroses em rela{\c{c}}{\~{a}}o {\`{a}}s m{\'{e}}dias dos pais e da cultivar-padr{\~{a}}o. Foram avaliadas nove caracter{\'{i}}sticas agron{\^{o}}micas – dias para a frutifica{\c{c}}{\~{a}}o (DF), n{\'{u}}mero total de frutos por planta (NTFP), peso m{\'{e}}dio do fruto (PMF), comprimento do fruto (CF), di{\^{a}}metro do fruto (DIAMF), espessura do pericarpo (EP), firmeza do fruto (FE), teor de s{\'{o}}lidos sol{\'{u}}veis totais (TSS) e produtividade (PROD) – em 17 tratamentos (cinco genitores, dez h{\'{i}}bridos e duas testemunhas comerciais – ‘Cait{\'{e}}' e ‘Imbuia') dispostos em blocos completos casualizados na Esta{\c{c}}{\~{a}}o Experimental da PESAGRO-Rio de Campos dos Goytacazes, no per{\'{i}}odo de junho a setembro de 2012. Os efeitos g{\^{e}}nicos n{\~{a}}o aditivos foram superiores aos efeitos aditivos para a produtividade (PROD), n{\'{u}}mero de frutos por planta (NTFP), peso m{\'{e}}dio do fruto (PMF) e firmeza (FE). Dos h{\'{i}}bridos avaliados, destacou-se como promissor para dupla finalidade – mesa e agroind{\'{u}}stria – a combina{\c{c}}{\~{a}}o ‘B13LD' x ‘Rio Grande', por revelar adequadas estimativas de CEC e de heterose para as caracter{\'{i}}sticas FE, TSS, NTFP e PROD.},
author = {da Gra{\c{c}}a, Ant{\'{o}}nio Jacinto Paulo and J{\'{u}}nior, Antonio Teixeira do Amaral and Rodrigues, Rosana and Gon{\c{c}}alves, Leandro S. A. and Sudr{\'{e}}, Cl{\'{a}}udia P. and Vivas, Marcelo and de Melo, Paulo C{\'{e}}sar T.},
doi = {10.1590/HB.V33I3.304},
issn = {S0102-0536},
journal = {Horticultura Brasileira},
month = {aug},
number = {3},
title = {{Heterose e capacidade combinat{\'{o}}ria de cultivares de tomateiro para dupla finalidade prospectadas para o mercado brasileiro e mo{\c{c}}ambicano}},
