file = {:Users/diegovidal/Documents/Mendeley Desktop/Zambrano et al. - 2014 - Determining the effectiveness of Candida guilliermondii in the biological control of Rhizopus stolonifer in pos.pdf:pdf},
number = {1},
pages = {51--62},
title = {{Determining the effectiveness of   Candida  guilliermondii  in the biological control of   Rhizopus stolonifer  in postharvest tomatoes}},
url = {http://www.scielo.org.co/pdf/unsc/v19n1/v19n1a04.pdf},
volume = {19},
year = {2014}
}
