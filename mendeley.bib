@article{Menezes2018,
abstract = {O tomate {\'{e}} uma das oler{\'{i}}colas de maior import{\^{a}}ncia econ{\^{o}}mica e maior prefer{\^{e}}ncia de consumo, no entanto devido a sua alta perecibilidade {\'{e}} fundamental o estudo da conserva{\c{c}}{\~{a}}o p{\'{o}}s-colheita com o uso dos revestimentos comest{\'{i}}veis. Diante disse o presente trabalho teve por objetivo avaliar o efeito da aplica{\c{c}}{\~{a}}o de diferentes revestimentos comest{\'{i}}veis a fim de determinar sua influ{\^{e}}ncia na preserva{\c{c}}{\~{a}}o da qualidade do tomate de mesa armazenados sob condi{\c{c}}{\~{a}}o de temperatura ambiente. O experimento foi conduzido em um delineamento inteiramente casualizado sob um arranjo fatorial 5x5 (cinco tempos de armazenamento: 0; 3; 6; 9 e 12 dias) e (cinco tratamentos: controle, f{\'{e}}cula de mandioca, amido de milho, cloreto de c{\'{a}}lcio e extrato de pr{\'{o}}polis na concentra{\c{c}}{\~{a}}o de 3{\%}). Em intervalos de cada tr{\^{e}}s dias os frutos foram avaliados quanto: perda de massa fresca, s{\'{o}}lidos sol{\'{u}}veis totais, acidez total titul{\'{a}}vel, pH, rela{\c{c}}{\~{a}}o SST/ATT e an{\'{a}}lise sensorial sobre os atributos firmeza, colora{\c{c}}{\~{a}}o da casca, qualidade geral e incid{\^{e}}ncia de podrid{\~{o}}es, al{\'{e}}m de an{\'{a}}lise de componentes principais. Houve efeito significativo ao n{\'{i}}vel de 1{\%} de probabilidade na intera{\c{c}}{\~{a}}o dos fatores dias de armazenamento e revestimentos. De modo geral os tratamentos com f{\'{e}}cula de mandioca e amido de milho respondem melhor ao controle das altera{\c{c}}{\~{o}}es fisiol{\'{o}}gicas e qualitativas que envolvem o amadurecimento dos tomates, sendo, portanto indicados como alternativas p{\'{o}}s-colheita para prolongar a vida {\'{u}}til desses frutos quando mantidos em condi{\c{c}}{\~{a}}o de temperatura ambiente de comercializa{\c{c}}{\~{a}}o e consumo.},
author = {Menezes, Keren Railka Paiva and Santos, Guilherme Celestino de Souza and de Oliveira, Olganeth Moreira and Sanches, Alex Guimar{\~{a}}es and Cordeiro, Carlos Alberto Martins and de Oliveira, Antonio Rafael Gomes},
