journal = {Revista de Ci{\^{e}}ncias Agroveterin{\'{a}}rias},
keywords = {Ralstonia solanacearum,Solanum lycopersicum L.,fruits.,frutos.,hormone,horm{\^{o}}nio},
month = {may},
number = {1},
pages = {54--60},
title = {{Indu{\c{c}}{\~{a}}o de toler{\^{a}}ncia {\`{a}} murcha bacteriana em h{\'{i}}bridos de tomates por aplica{\c{c}}{\~{a}}o de giberelina}},
