issn = {S0102-0536},
journal = {Horticultura Brasileira},
number = {3},
title = {{Heterose e capacidade combinat{\'{o}}ria de cultivares de tomateiro para dupla finalidade prospectadas para o mercado brasileiro e mo{\c{c}}ambicano}},
url = {http://www.ashbmcd.com/index.php/HB/article/view/304},
volume = {33},
