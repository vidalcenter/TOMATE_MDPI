@article{Guilherme2014,
abstract = {Este trabalho avaliou aspectos sensoriais e caracter{\'{i}}sticas f{\'{i}}sico-qu{\'{i}}micas de dois gen{\'{o}}tipos n{\~{a}}o-comerciais de tomate cereja, CH152 e CLN1561, comparando-os ao gen{\'{o}}tipo Carolina, todos cultivados organicamente. Frutos maduros das tr{\^{e}}s variedades foram colhidos na mesma {\'{e}}poca, selecionados, higienizados e submetidos {\`{a}} an{\'{a}}lise sensorial por 50 provadores adultos, que usando uma escala hed{\^{o}}nica de 9 pontos (de desgostar muit{\'{i}}ssimo a gostar muit{\'{i}}ssimo) classificaram os frutos em termos de aroma, sabor, cor da polpa e aspecto geral. Nas an{\'{a}}lises f{\'{i}}sico-qu{\'{i}}micas, determinou-se pH, s{\'{o}}lidos sol{\'{u}}veis (SS) e acidez titul{\'{a}}vel (AT). Para os atributos aroma e aspecto geral, as 3 variedades obtiveram escores m{\'{e}}dios de 7,0 a 7,9, o que {\'{e}} correspondente {\`{a}} classifica{\c{c}}{\~{a}}o “gostar moderadamente”; e escore de 8,0 a 8,9 para o atributo cor, equivalente a “gostar muito”. Para o quesito sabor, tomates “Carolina” e CLN1561A obtiveram o escore correspondente a “gostar moderadamente”, e CH152 foi relacionado a “gostar muito”. Os valores m{\'{e}}dios obtidos para os par{\^{a}}metros f{\'{i}}sico-qu{\'{i}}micos (pH = 4,41 a 4,61; SS = 4,03 a 5,06; e AT = 335,13 a 369,23) foram semelhantes entre as variedades (ANOVA, P{\textgreater}0,05). Conclui-se que ambas as variedades testadas, CH152 e CLN1561, produzem frutos de alta qualidade sensorial e f{\'{i}}sico-qu{\'{i}}mica tendo potencial para serem exploradas comercialmente.},
author = {Guilherme, Denilson De Oliveira and {De Pinho}, Lucin{\'{e}}ia and {Menezes Cavalcanti}, Th{\^{a}}mara Figueiredo and {Da Costa}, C{\^{a}}ndido Alves and {De Almeida}, Anna Christina},
file = {:Users/diegovidal/Documents/Mendeley Desktop/2683-12007-1-PB.pdf:pdf},
issn = {19832125},
journal = {Revista Caatinga},
keywords = {Flavor,Lycopersion esculentum,Organic crop,Quality},
number = {1},
pages = {181--186},
title = {{An{\'{a}}lise sensorial e f{\'{i}}sico-qu{\'{i}}mica de frutos tomate cereja org{\^{a}}nicos}},
volume = {27},
year = {2014}
}
@article{Souza2013,
abstract = {O presente trabalho foi realizado na Floresta Estadual do Palmito, Paranagu{\'{a}}, PR, em {\'{a}}rea coberta originalmente com Floresta Ombr{\'{o}}fila Densa das Terras Baixas, que sofreu processo de degrada{\c{c}}{\~{a}}o antr{\'{o}}pica, apresentando, hoje, {\'{a}}reas representativas de diversas fases da regenera{\c{c}}{\~{a}}o secund{\'{a}}ria. Esta pesquisa visou avaliar respostas de microrganismos {\`{a}}s mudan{\c{c}}as ambientais da floresta em processo de regenera{\c{c}}{\~{a}}o. Foram selecionadas tr{\^{e}}s {\'{a}}reas, representativas de diferentes fases da sucess{\~{a}}o secund{\'{a}}ria deste ecossistema, onde foram coletadas amostras dos horizontes superficiais do solo: F, H e A, nas quatro esta{\c{c}}{\~{o}}es clim{\'{a}}ticas. Nas amostras coletadas, a avalia{\c{c}}{\~{a}}o da atividade microbiana foi feita pela quantifica{\c{c}}{\~{a}}o da respira{\c{c}}{\~{a}}o microbiana. Os resultados mostraram que a produ{\c{c}}{\~{a}}o de CO2 nos horizontes do solo foi decrescente no seguinte sentido: F{\textgreater}H{\textgreater}A. Foi tamb{\'{e}}m superior na fase avan{\c{c}}ada da vegeta{\c{c}}{\~{a}}o, sobretudo no horizonte F. As esta{\c{c}}{\~{o}}es do ano n{\~{a}}o mostraram efeito conclusivo sobre a atividade microbiana. A melhoria da qualidade do solo, ao longo da evolu{\c{c}}{\~{a}}o florestal, foi caracterizada indiretamente atrav{\'{e}}s da determina{\c{c}}{\~{a}}o da respira{\c{c}}{\~{a}}o microbiana, o que sugere esta an{\'{a}}lise como ferramenta indicadora de qualidade de s{\'{i}}tio em ecossistemas florestais. Microbial respiration as indicator of soil quality in forest ecosystem Abstract This research was carried out at the State Forest of Palmito, Paranagu{\'{a}}, PR, Brazil, originally covered with Tropical Atlantic Rainforest. This forest has suffered anthropic degradation, and the goal of this research was to evaluate the response of microorganisms to forest environment changes in the process of regeneration. Three representative areas of the secondary succession of this ecosystem have been selected, where samples of the superficial layer of the soil were collected at the four climatic seasons. The samples were taken from the following soil horizons: F, H and A. The evaluation of the microbial activity was made by the quantification of microbial respiration. The results shown the production of CO2 on the following sequence: F{\textgreater}H{\textgreater}A. And also superior at the advanced phase of the ecological succession, specially at F horizon. Climatic seasons showed no conclusive effect on the microbial activity. Soil quality amelioration along the forest evolution was characterized by determination of the microbial respiration, suggesting this analyses as an indicator tool of forest site quality.},
author = {Flach, Matheus Klein and Buske, Taise Cristine and Marques, Renato and Jahnel, Marcelo Cabral and Souza, E.J. and Cunha, Fernando Fran{\c{c}}a and Magalh{\~{a}}es, F.F. and Silva, T.R. and Borges, M.C.R.Z. and Roque, C.G. and Oliveira, Flavio Gon{\c{c}}alves},
doi = {10.5380/rf.v35i1.2435},
