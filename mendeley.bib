doi = {10.1016/j.optlaseng.2013.06.017},
file = {:Users/diegovidal/Documents/Mendeley Desktop//Zdunek et al. - 2014 - The biospeckle method for the investigation of agricultural crops A review.pdf:pdf},
issn = {01438166},
keywords = {Biospeckle,Fruit,Nondestructive,Quality},
number = {1},
pages = {276--285},
publisher = {Elsevier},
title = {{The biospeckle method for the investigation of agricultural crops: A review}},
