journal = {Chemosphere},
keywords = {Botrytis cinerea,Copper,Cu-oxalate,Detoxification mechanisms,Metal oxide nanoparticles,Metal pollution},
pages = {178--185},
title = {{Biotransformation of copper oxide nanoparticles by the pathogenic fungus Botrytis cinerea}},
volume = {180},
