@misc{Arefi2016a,
abstract = {An apple that is soft and lacks juiciness during consumption is characterized as a mealy fruit. Since mealiness in apple is considered as an undesirable quality parameter, this work aimed at developing classification models based on biospeckle imaging in order to recognize mealy from non-mealy apples. To evaluate the potential of biospeckle imaging in detection of mealiness, a total of 760 'Red Delicious' apples were harvested. The 540 samples were stored under cold conditions for various time periods from 0 to 5 months. The remaining samples were kept at 20°C and 95{\{}{\%}{\}} relative humidity for 10-26 days. Biospeckle images of each apple were acquired at wavelengths of 680 and 780nm, respectively. Biospeckle imaging was immediately followed by a confined compression test to measure fruit stiffness and juiciness. These parameters were used to categorize the samples into three classes called fresh, semi-mealy, and mealy. Results of clustering based on a self-organizing map showed that apple fruit are mealy when their stiffness and juiciness are less than 20kNm-1 and 5cm2, respectively. After classification of apples by destructive methods, time-historical speckle patterns were configured and biospeckle features such as the inertia moment, the absolute value of differences, and autocorrelation were extracted. Results showed that biospeckle activity for fresh samples was higher than semi-mealy and mealy. Finally, several neural network models were developed to classify apples. First, classification of apples into mealy and non-mealy classes was carried out, and then non-mealy apples were classified into fresh and semi-mealy classes. The best classification accuracy for fresh (81.7{\{}{\%}{\}}) and semi-mealy (70.9{\{}{\%}{\}}) apples were achieved at 780nm. However, much better result (77.3{\{}{\%}{\}} accuracy) for classification of mealy apples was observed at the wavelength of 680nm.},
author = {Arefi, Arman and {Ahmadi Moghaddam}, Parviz and Hassanpour, Ali and Mollazade, Kaveh and {Modarres Motlagh}, Asad},
booktitle = {Postharvest Biology and Technology},
doi = {10.1016/j.postharvbio.2015.09.001},
file = {:Users/diegovidal/Documents/Mendeley Desktop//Arefi et al. - 2016 - Non-destructive identification of mealy apples using biospeckle imaging.pdf:pdf},
issn = {09255214},
keywords = {Confinded compression test,Dynamic speckle pattern,Laser,Mealiness,Non-invasive test},
pages = {266--276},
title = {{Non-destructive identification of mealy apples using biospeckle imaging}},
url = {http://www.profigen.com.br},
volume = {112},
year = {2016}
}
@article{Moreira2014,
abstract = {Biospeckle (BSL) is a phenomenon that develops when a dynamic process occurs under laser illumination in a material containing considerable information on biological and non-biological activity. Analysis of such data has been a challenge because of complex interaction between light and material, and therefore, of high sensitivity of the BSL and variability of biological material. This study was aimed at improving the robustness of BSL techniques and thus reducing its subjectivity and dependence on human expertise. The work presents a protocol that aids in a quality test of three key features of the speckle patterns, particularly those related to saturation, under-exposure, homogeneity and contrast of the grains. The quality test was carried out with data from a maize seed and a paint drying under biospeckle laser monitoring, where it was possible to identify some problems in the images related to the tested features. The results establish the feasibility of the quality test control, which form the early steps of the biospeckle laser analysis, and serves in helping the judgment of the setup. {\textcopyright} 2014 Elsevier Ltd.},
author = {Moreira, Junio and Cardoso, R. R. and Braga, R. A.},
doi = {10.1016/j.optlaseng.2014.04.005},
issn = {01438166},
journal = {Optics and Lasers in Engineering},
title = {{Quality test protocol to dynamic laser speckle analysis}},
volume = {61},
year = {2014}
}
@article{GouveadeSouza2016,
abstract = {Rev. Bras. Reprod. Anim., Belo Horizonte, v.41, n.3, p.671-677, jul./set. 2017. Dispon{\'{i}}vel em www.cbra.org.br {\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_}{\_} Recebido: 23 de novembro de 2016 Aceito: 5 de julho de 2017 Produ{\c{c}}{\~{a}}o in vitro de embri{\~{o}}es bovinos com s{\^{e}}men congelado ap{\'{o}}s pr{\'{e}}via infec{\c{c}}{\~{a}}o com Herpesvirus bovino tipo 5 In vitro production of bovine embryos with frozen semen previously infected with Bovine Herpesvirus type 5 before sperm freezing Resumo O Herpesvirus bovino tipo 5 (BoHV-5) tem sido recentemente descrito no s{\^{e}}men contaminado. Apesar de ter sido identificado no s{\^{e}}men bovino, sua influ{\^{e}}ncia na produ{\c{c}}{\~{a}}o de embri{\~{o}}es in vitro n{\~{a}}o foi ainda relatada. O presente estudo teve como objetivo verificar a susceptibilidade de o{\'{o}}citos bovinos maturados e fertilizados in vitro, com s{\^{e}}men congelado, infectado experimentalmente com BoHV-5, antes da congela{\c{c}}{\~{a}}o, avaliando-se o desenvolvimento embrion{\'{a}}rio subsequente e o poss{\'{i}}vel carreamento do v{\'{i}}rus para o embri{\~{a}}o via espermatozoide. Ejaculados de touro foram misturados e divididos em dois grupos: Grupo I, s{\^{e}}men n{\~{a}}o exposto ao v{\'{i}}rus e, Grupo II, s{\^{e}}men exposto ao v{\'{i}}rus. A presen{\c{c}}a do BoHV-5 foi investigada atrav{\'{e}}s de PCR e teste de hibridiza{\c{c}}{\~{a}}o in situ (ISH). Os dados foram analisados atrav{\'{e}}s de Teste-t de Student com P {\textless} 0,05 considerado como significativo. O v{\'{i}}rus foi detectado em embri{\~{o}}es avaliados com 168 h p{\'{o}}s-insemina{\c{c}}{\~{a}}o. N{\~{a}}o houve diferen{\c{c}}a significativa entre os diferentes grupos de tratamento para as taxas de produ{\c{c}}{\~{a}}o in vitro de embri{\~{o}}es para os respectivos est{\'{a}}gios de desenvolvimento. Assim, conclui-se que, o s{\^{e}}men contaminado com o BoHV-5 pode infectar os o{\'{o}}citos durante a fertiliza{\c{c}}{\~{a}}o in vitro, com carreamento do v{\'{i}}rus para os embri{\~{o}}es, n{\~{a}}o afetando, por{\'{e}}m, o seu desenvolvimento in vitro. Palavras-chave: BoHV-5, preserva{\c{c}}{\~{a}}o do s{\^{e}}men, hibridiza{\c{c}}{\~{a}}o in situ fluorescente, desenvolvimento embrion{\'{a}}rio. Abstract Bovine herpesvirus type 5 (BoHV-5) has been recently described from contaminated semen. Despite having been identified in bovine semen, its transmission through use of infected fresh semen, just before freezing, on the in vitro embryo production has not yet been reported so far. Thus, this study was carried out to investigate the susceptibility of matured bovine oocytes to BoHV-5 infection after using frozen semen previously exposed to the virus, and their ability to develop for advanced blastocyst stages following in vitro fertilization (IVF). Ejaculates from bulls were mixed and divided into two groups, as follows: Group 1, semen not exposed to the virus, and Group 2, infected sperm. The presence of BoHV-5 was investigated by PCR and in situ hybridization assays. Results were analyzed through use of Student t -test with P {\textless} 0.05 considered as significant. The virus was detected in blastocysts evaluated at 168 h post-insemination. There was no significant difference between groups for the in vitro embryo production at any developmental stage. Thus, it is concluded that semen infected with BoHV-5 can infect oocytes during IVF, with the virus transmission to embryos via sperm; however, embryonic development was not impaired, as evidenced by the subsequent blastocyst formation.},
author = {{Gouv{\^{e}}a de Souza}, Diego and Silva-Frade, Camila and Calegari, Renata S and Crespilho, Andr{\'{e}} M and Paschoal, Daniela M and Sudano, Mateus J and Marqui, Fernanda N and {Martins Jr}, Alicio and Cardoso, Tereza C},
file = {:Users/diegovidal/Documents/Mendeley Desktop/Gouv{\^{e}}a de Souza et al. - Unknown - Produ{\c{c}}{\~{a}}o in vitro de embri{\~{o}}es bovinos com s{\^{e}}men congelado ap{\'{o}}s pr{\'{e}}via infec{\c{c}}{\~{a}}o c om Herpesvi.pdf:pdf},
journal = {Rev. Bras. Reprod. Anim.},
pages = {671--677},
title = {{Produ{\c{c}}{\~{a}}o in vitro de embri{\~{o}}es bovinos com s{\^{e}}men congelado ap{\'{o}}s pr{\'{e}}via infec{\c{c}}{\~{a}}o c om Herpesvirus bovino tipo 5}},
url = {http://www.cbra.org.br/portal/downloads/publicacoes/rbra/v41/n3/p671-677 (RB626).pdf},
